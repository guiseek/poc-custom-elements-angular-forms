{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "allowUnreachableCode": false,
    "declaration": false,
    "esModuleInterop": true,
    "lib": ["es2017"],
    "moduleResolution": "node",
    "pretty": true,
    "resolveJsonModule": true,
    "strict": true,
    "outDir": "../dist/out-tsc/tools",
    "rootDir": ".",
    "module": "commonjs",
    "target": "es5",
    "types": ["node"]
  },
  "include": ["**/*.ts"]
}
